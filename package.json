{
    "name": "laravel-restype-client",
    "version": "0.1.0",
    "description": "Api client that implements typescript api definitions that are automatically generated via laravel-restype plugin.",
    "main": "dist/esm/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "browser": "./dist/esm/index.js",
            "default": "./dist/esm/index.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "rollup -c",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/laravel-restype/laravel-restype-client.git"
    },
    "author": "Andrei Telteu",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/laravel-restype/laravel-restype-client/issues"
    },
    "homepage": "https://github.com/laravel-restype/laravel-restype-client#readme",
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.0.0",
        "rollup": "^2.79.1",
        "typescript": "^4.9.4"
    },
    "keywords": [
        "laravel",
        "typescript",
        "rest",
        "api",
        "documentation"
    ],
    "dependencies": {
        "tslib": "^2.4.1"
    }
}
